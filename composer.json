{
    "name": "agoalofalife/bpm-online",
    "description": "Using the API BPM Terrasoft",
    "type": "no",
    "license": "MIT",
    "version" : "1.0.0",
    "minimum-stability": "dev",
    "keywords": [
        "bpm-online",
        "terrasoft-http",
        "bpm",
        "terrasoft"
    ],
    "require": {
        "beberlei/assert": "^2.5",
        "illuminate/container": "^5.5",
        "illuminate/support": "^5.5",
        "illuminate/config": "^5.5@dev",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.6",
        "mockery/mockery": "^0.9.9",
        "phpunit/phpunit": "^6"
    },
    "autoload": {
        "psr-4": {
            "agoalofalife\\bpmOnline\\": "",
            "agoalofalife\\bpm\\" : "src"
        },
        "files": [
            "helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "agoalofalife\\Tests\\": "tests/"
        }
    },
        "scripts": {
        "test": "vendor/bin/phpunit --stop-on-failure --no-coverage"
    }
}
